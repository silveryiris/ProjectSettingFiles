{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "allowImportExportEverywhere": true,
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "plugins": ["vue"],
  "extends": ["eslint:recommended", "plugin:vue/base"],
  "rules": {
    "no-console": 0
  }
}
